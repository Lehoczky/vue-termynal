{
  "name": "@lehoczky/vue-termynal",
  "description": "Vue.js implementation of the termynal library.",
  "version": "0.1.4",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src/ --ext .js,.vue --max-warnings 0",
    "styles:check": "stylelint \"src/**/*.css\"",
    "styles:fix": "stylelint \"src/**/*.css\" --fix",
    "format:check": "prettier src/** --check",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-termynal.es.js",
      "require": "./dist/vue-termynal.umd.js"
    }
  },
  "dependencies": {
    "style-inject": "^0.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-vue": "^7.15.1",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vite": "^2.4.4",
    "vue": "^3.0.5",
    "vue-tsc": "^0.2.2",
    "vuepress-vite": "^2.0.0-beta.24"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "author": {
    "name": "Zoltan Lehoczky",
    "email": "ifjlehoczkyzoltan@gmail.com",
    "url": "https://github.com/Lehoczky"
  },
  "repository": {
    "url": "https://github.com/Lehoczky/vue-termynal",
    "type": "git"
  }
}
